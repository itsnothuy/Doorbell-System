{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "DEVELOPMENT_MODE": "true",
    "VERCEL": "true"
  },
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  }
}
